{
  "manifest_version": 2,
  "name": "Cream",
  "version": "0.1.0",
  "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+K",
          "mac": "MacCtrl+Shift+K"
        }
      }
  },
  "background": {
    "scripts": [
      "watch.js", "helpers.js", "main.js", "background.js", "listeners.js"
      ],
    "persistent": false
  },
  "icons":{
    "16": "prince_icon.png",
		"48": "prince_icon.png"
  },
  "browser_action": {
   "default_title": "Follow the noize",
   "default_icon": "prince_icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
